          <DataField>Key Task</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TaskDuration">
          <DataField>TaskDuration</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Milestone_Flag">
          <DataField>Milestone Flag</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TaskIsMilestone">
          <DataField>TaskIsMilestone</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TaskDeliverableStartDate">
          <DataField>TaskDeliverableStartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Deliverable_Flag">
          <DataField>Deliverable Flag</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TaskActualFinishDate">
          <DataField>TaskActualFinishDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
