  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>PerViewDataMartNew</DataSourceName>
        <CommandText>DECLARE @ServerName VARCHAR(100) = @@ServerName
declare @ParmProjectName varchar(max) =N'fab43168-96d2-42fc-b3da-ea50e5c4a7bd,81849773-becb-e811-811e-0050568f18e4,1c3faa6a-4bce-e811-8128-0050568f3b8a,04c06799-0dd3-e811-8128-0050568f3b8a'
declare @ParmReportingFinish date ='2018-10-19 00:00:00'
declare @UpcomingPeriod VARCHAR(100) = N'15'
declare @ParmBaselineNonBaseline VARCHAR(100) =N'TaskBaselineFinish'
declare @ParmIncludePlanView VARCHAR(100) =N'Yes'

	--SELECT @ServerName
	
	-------------------------
	--Projects Table
	-------------------------
	IF OBJECT_ID('tempdb..#temp_Projects') IS NOT NULL
		DROP TABLE #temp_Projects

	SELECT Value AS [ProjectUID] INTO #temp_Projects FROM PerViewDataMart.dbo.ufnsplitstring((@ParmProjectName),',')
	
	-- perview data first

	IF OBJECT_ID('tempdb..#temp_perviewProjects') IS NOT NULL
		DROP TABLE #temp_perviewProjects

 
	SELECT	t1.* into #temp_perviewProjects
	FROM	(-- t1
			SELECT	p.ProjectStartDate AS [Start]
				  , p.ProjectFinishDate AS [Finish]
				  , p.[Upcoming Period]
				  , p.ProjectUID
				  , p.puid 
				  , p.ProjectName
				  --, 'https://' + 
						--CASE @ServerName 
						--	WHEN 'NADCWPDBSMSP01' THEN 'perview'
						--	WHEN 'NADCWQDBSMSP01' THEN 'perviewqa'
						--END
						--+ '.app.medcity.net/PWA/Project Detail Pages/project%20plan.aspx?ProjUid%3D' + CAST(p.ProjectUID AS VArChar (50)) AS ProjectURL
				  , PerViewDataMart.dbo.[udfGetProjectURL](p.ProjectUID) AS ProjectURL		
				  , ISNULL(p.[Project Client], N'&lt;No Project Client&gt;') AS [Project Client]
				  , p.[Project Client Display]
				  , CASE WHEN p.[Project Client] = 'Parallon' THEN 'Parallon' 
						ELSE ISNULL(p.[Level 2] + ' - ' + p.[Project Client Display], '&lt;No Project Client&gt;') 
					END AS [Client]
				  , ISNULL(p.[Project Category], N'&lt;No Project Category&gt;') AS [ProjectCategory]
				  , ISNULL(p.[Functional Area], N'&lt;No Functional Area&gt;') AS [Functional Area]
				  , p.[Line of Business] AS [LineOfBusiness]
				  , p.[Product line] AS [Product Line]

				  , p.[Level 2]
				  , ISNULL(p.[Business Owner], N'&lt;No Business Owner&gt;') AS [Business Owner]
				  , p.ProjectOwnerName AS [ProjectManager]
				  , ISNULL(p.[Project Sponsor],N'&lt;No Project Sponsor&gt;') AS [Project Sponsor]
				  , p.[Project Active Status] AS [Active Status]
				  , p.epm_platform as [epm_platform]
				  , p.[PM Allocation]
				  , ISNULL(p.[Project Priority], 'N/A') AS [Project Priority]
				  , ISNULL(p.[PM Assessed Project Health], '-') AS [PM Assessed Project Health]
				  , p.[Project Outcomes Expected]
				  , PerViewDataMart.dbo.udfStripXML(p.[Executive Status Summary]) AS [Executive Status Summary]  
				  , t.TaskName
				  , t.TaskPercentCompleted
				  , t.TaskBaseline0FinishDate AS [TaskBaselineFinish]
				  , t.[Key Task]
				  , t.TaskDuration
				  , t.[Milestone Flag]
				  , t.[TaskIsMilestone]
				  , t.TaskDeliverableStartDate
				  , t.[Deliverable Flag]
				  , t.TaskActualFinishDate
				  , a.[Task Comments_T] AS AssignmentTaskComments
				  , t.[Task Comments] AS TaskComments
				  , 1 as sortOrder
		
				  -- 08/11/2014 - Added Baseline and Non-Baseline option to report
				  , CASE @ParmBaselineNonBaseline
						WHEN 'TaskBaselineFinish' THEN
							CASE 
								-- 08/12/2015 - David Trussa - Changed logic for Upcoming Task Flag to split out Key Tasks, Milestones and Deliverables
								--WHEN (t.TaskBaseline0FinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.[Key Task] = 'Yes') OR (t.TaskDuration = 0) OR (t.[Milestone Flag] = 'Yes') OR (t.TaskDeliverableStartDate IS NOT NULL) OR (t.[Deliverable Flag] = 'Yes')) THEN 1
								WHEN (t.TaskBaseline0FinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND (t.[Key Task] = 'Yes') THEN 3 --Key Task
								WHEN (t.TaskBaseline0FinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.TaskDuration = 0) OR (t.[Milestone Flag] = 'Yes')) THEN 2 --MilestoneTask
								WHEN (t.TaskBaseline0FinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.TaskDeliverableStartDate IS NOT NULL) OR (t.[Deliverable Flag] = 'Yes')) THEN 1 --DeliverableTask
								--
								WHEN (t.TaskBaseline0FinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1)  THEN 0 --All Tasks
								--ELSE 2
								ELSE 4
							END
						WHEN 'TaskFinishDate' THEN
							CASE
								-- 08/12/2015 - David Trussa - Changed logic for Upcoming Task Flag to split out Key Tasks, Milestones and Deliverables
								--WHEN (t.TaskFinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.[Key Task] = 'Yes') OR (t.TaskDuration = 0) OR (t.[Milestone Flag] = 'Yes') OR (t.TaskDeliverableStartDate IS NOT NULL) OR (t.[Deliverable Flag] = 'Yes')) THEN 1
								WHEN (t.TaskFinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND (t.[Key Task] = 'Yes') THEN 3 --Key Task
								WHEN (t.TaskFinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.TaskDuration = 0) OR (t.[Milestone Flag] = 'Yes')) THEN 2 --MilestoneTask
								WHEN (t.TaskFinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.TaskDeliverableStartDate IS NOT NULL) OR (t.[Deliverable Flag] = 'Yes')) THEN 1 --DeliverableTask
								--
								WHEN (t.TaskFinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1)  THEN 0 --All Tasks
								--ELSE 2
								ELSE 4
							END
					END AS UpcomingTaskFlag
				  , t.TaskFinishDate
				  -- 08/05/2015 - David Trussa - Added ProjectPercentCompleted, PM Notes and FDI Flag
				  , p.ProjectPercentCompleted
				  , PerViewDataMart.dbo.udfStripXML(p.[Project Manager Notes]) AS [PM Notes]
				  , p.FDI
				  -- 09/14/2015 - David Trussa - Added Service Line, Portfolio Funding Source
				  , ISNULL(p.[Service Line],'&lt;No Service Line&gt;') AS [Service Line]
				  , ISNULL(p.[Portfolio Funding Source],'N/A') AS [Portfolio Funding Source] 

			FROM	#temp_Projects tpj
					INNER JOIN PerViewDataMart.dbo.dim_Project p WITH (NOLOCK)
						ON tpj.ProjectUID = p.ProjectUID
					LEFT OUTER JOIN PerViewDataMart.dbo.dim_task AS t WITH (NOLOCK)
						ON p.ProjectUID = t.ProjectUID
					LEFT OUTER JOIN PerViewDataMart.dbo.dim_assignment a WITH (NOLOCK)
						ON a.TaskUID = t.TaskUID
			WHERE	1=1
					AND (p.ProjectType IN (0,5,6))

			) t1
	WHERE	1=1
	ORDER BY [Project Client], [Project Priority], [ProjectName] 

	--select * from  #temp_perviewProjects

	--- get puid's of planview projects associated

	IF OBJECT_ID('tempdb..#temp_planviewProjectIDs') IS NOT NULL
		DROP TABLE #temp_planviewProjectIDs
		create TABLE #temp_planviewProjectIDs (
			puid varchar(42) null
			, perviewUID uniqueidentifier null
			)

	if @ParmIncludePlanView = 'No'  
			
		insert into #temp_planviewProjectIDs 
			select 
			cast (sys.fn_varbintohexsubstring(0, HashBytes('SHA1', CAST('0000000' AS char(40))), 1, 0) as char(42) )  as puid 
			,   '002b8edf-b6a5-4e6d-8029-b9a5a0cb2d8c' as perviewUID   ;

	else 
		insert into #temp_planviewProjectIDs
			SELECT cast (sys.fn_varbintohexsubstring(0, HashBytes('SHA1', CAST(ppl_code AS char(40))), 1, 0) as char(42) )  as puid	 
			, ProjectGuid as perviewUID
			from mapperDB_test.dbo.PlanViewProjects ass
			where ass.ProjectGuid in (select * from #temp_Projects) ;
		

	-- get status info of associated planView projects

	IF OBJECT_ID('tempdb..#temp_planviewProjects') IS NOT NULL
		DROP TABLE #temp_planviewProjects

   --select  *  from #temp_planviewProjectIDs

	SELECT	t1.* into #temp_planviewProjects
	FROM	(-- t1
			SELECT	p.ProjectStartDate AS [Start]
				  , p.ProjectFinishDate AS [Finish]
				  , p.[Upcoming Period]
				  , pids.perviewUID   -- get the mapped perviewUID here
				  , p.puid 
				  , p.ProjectName
				  --, 'https://' + 
						--CASE @ServerName 
						--	WHEN 'NADCWPDBSMSP01' THEN 'perview'
						--	WHEN 'NADCWQDBSMSP01' THEN 'perviewqa'
						--END
						--+ '.app.medcity.net/PWA/Project Detail Pages/project%20plan.aspx?ProjUid%3D' + CAST(p.ProjectUID AS VArChar (50)) AS ProjectURL
				  , '' AS ProjectURL		
				  , (select top 1 per.[Project Client] from #temp_perviewProjects per where per.ProjectUID = pids.perviewUID )  			AS [Project Client]
				  , (select top 1  per.[Project Client Display] from #temp_perviewProjects per where per.ProjectUID = pids.perviewUID)                  as [Project Client Display]
				  , ( select top 1 per.Client from #temp_perviewProjects per where per.ProjectUID = pids.perviewUID      )	AS [Client]
				  , ( select top 1 per.ProjectCategory from #temp_perviewProjects per where per.ProjectUID = pids.perviewUID      )	 AS [ProjectCategory]
				  , ( select top 1 per.[Functional Area]  from #temp_perviewProjects per where per.ProjectUID = pids.perviewUID      )	AS [Functional Area]
				  , ( select top 1  per.[LineOfBusiness]  from #temp_perviewProjects per where per.ProjectUID = pids.perviewUID      )		AS [LineOfBusiness]
				  , ( select top 1  per.[Product Line]  from #temp_perviewProjects per where per.ProjectUID = pids.perviewUID      )			AS [Product Line]

				  , ( select top 1  per.[Level 2]  from #temp_perviewProjects per where per.ProjectUID = pids.perviewUID      )   as [Level 2]
				  , ISNULL(p.[Business Owner], N'&lt;No Business Owner&gt;') AS [Business Owner]
				  , p.ProjectOwnerName AS [ProjectManager]
				  , ( select top 1 per.[Project Sponsor]  from #temp_perviewProjects per where per.ProjectUID = pids.perviewUID      )		AS [Project Sponsor]
				  , p.[Project Active Status] AS [Active Status]
				  , p.epm_platform as [epm_platform]
				  , p.[PM Allocation]
				  , ISNULL(p.[Project Priority], 'N/A') AS [Project Priority]
				  , ISNULL(p.[PM Assessed Project Health], '-') AS [PM Assessed Project Health]
				  , p.[Project Outcomes Expected]
				  , PerViewDataMart.dbo.udfStripXML(p.[Executive Status Summary]) AS [Executive Status Summary]  
				  , t.TaskName
				  , t.TaskPercentCompleted
				  , t.TaskBaseline0FinishDate AS [TaskBaselineFinish]
				  , t.[Key Task]
				  , t.TaskDuration
				  , t.[Milestone Flag]
				  , t.[TaskIsMilestone]
				  , t.TaskDeliverableStartDate
				  , t.[Deliverable Flag]
				  , t.TaskActualFinishDate
				  , a.[Task Comments_T] AS AssignmentTaskComments
				  , t.[Task Comments] AS TaskComments
				  , 2 as sortorder
		
				  -- 08/11/2014 - Added Baseline and Non-Baseline option to report
				  , CASE @ParmBaselineNonBaseline
						WHEN 'TaskBaselineFinish' THEN
							CASE 
								-- 08/12/2015 - David Trussa - Changed logic for Upcoming Task Flag to split out Key Tasks, Milestones and Deliverables
								--WHEN (t.TaskBaseline0FinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.[Key Task] = 'Yes') OR (t.TaskDuration = 0) OR (t.[Milestone Flag] = 'Yes') OR (t.TaskDeliverableStartDate IS NOT NULL) OR (t.[Deliverable Flag] = 'Yes')) THEN 1
								WHEN (t.TaskBaseline0FinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND (t.[Key Task] = 'Yes') THEN 3 --Key Task
								WHEN (t.TaskBaseline0FinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.TaskDuration = 0) OR (t.[Milestone Flag] = 'Yes')) THEN 2 --MilestoneTask
								WHEN (t.TaskBaseline0FinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.TaskDeliverableStartDate IS NOT NULL) OR (t.[Deliverable Flag] = 'Yes')) THEN 1 --DeliverableTask
								--
								WHEN (t.TaskBaseline0FinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1)  THEN 0 --All Tasks
								--ELSE 2
								ELSE 4
							END
						WHEN 'TaskFinishDate' THEN
							CASE
								-- 08/12/2015 - David Trussa - Changed logic for Upcoming Task Flag to split out Key Tasks, Milestones and Deliverables
								--WHEN (t.TaskFinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.[Key Task] = 'Yes') OR (t.TaskDuration = 0) OR (t.[Milestone Flag] = 'Yes') OR (t.TaskDeliverableStartDate IS NOT NULL) OR (t.[Deliverable Flag] = 'Yes')) THEN 1
								WHEN (t.TaskFinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND (t.[Key Task] = 'Yes') THEN 3 --Key Task
								WHEN (t.TaskFinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.TaskDuration = 0) OR (t.[Milestone Flag] = 'Yes')) THEN 2 --MilestoneTask
								WHEN (t.TaskFinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1) AND ((t.TaskDeliverableStartDate IS NOT NULL) OR (t.[Deliverable Flag] = 'Yes')) THEN 1 --DeliverableTask
								--
								WHEN (t.TaskFinishDate &gt;= @ParmReportingFinish) AND (t.TaskFinishDate &lt;= DateAdd( d , Cast(@UpcomingPeriod AS Integer), @ParmReportingFinish)) AND 
									 (t.TaskPercentCompleted &lt; 100) AND (t.TaskIsActive = 1)  THEN 0 --All Tasks
								--ELSE 2
								ELSE 4
							END
					END AS UpcomingTaskFlag
				  , t.TaskFinishDate
				  -- 08/05/2015 - David Trussa - Added ProjectPercentCompleted, PM Notes and FDI Flag
				  , p.ProjectPercentCompleted
				  , PerViewDataMart.dbo.udfStripXML(p.[Project Manager Notes]) AS [PM Notes]
				  , p.FDI
				  -- 09/14/2015 - David Trussa - Added Service Line, Portfolio Funding Source
				  , ISNULL(p.[Service Line],'&lt;No Service Line&gt;') AS [Service Line]
				  , ISNULL(p.[Portfolio Funding Source],'N/A') AS [Portfolio Funding Source] 

			from #temp_planviewProjectIDs pids
			
					left join dim_project p on pids.puid = p.puid 
					LEFT OUTER JOIN PerViewDataMart.dbo.dim_task AS t WITH (NOLOCK)
						ON p.ProjectUID = t.ProjectUID
					LEFT OUTER JOIN PerViewDataMart.dbo.dim_assignment a WITH (NOLOCK)
						ON a.TaskUID = t.TaskUID
			WHERE	1=1
					and p.puid in (select puid from #temp_planviewProjectIDs) 
					AND (p.ProjectType IN (0,5,6))

			) t1

--SELECT	* from #temp_perviewProjects 
--select * from #temp_planviewProjects


		IF OBJECT_ID('tempdb..#CombinedProjects') IS NOT NULL
		DROP TABLE #CombinedProjects


SELECT	cprojs.* into #CombinedProjects 
	FROM	(-- t1
			SELECT	* from #temp_perviewProjects 
			union all select * from #temp_planviewProjects
				  ) cprojs

	select * from  #CombinedProjects cp
	order by cp.ProjectUID, cp.sortOrder, cp.ProjectName  asc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
